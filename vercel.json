{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "routes": [
    {
      "src": "/.*%7C.*",
      "mitigate": {
        "action": "deny"
      }
    },
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*Chrome/125.*"
        }
      ],
      "mitigate": {
        "action": "deny"
      }
    },
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(bot|crawler|spider|scrapy|wget|curl|python|java|headless|selenium|puppeteer|playwright|okhttp|axios|scraper|semrush|ahrefs|mj12bot|dotbot|blexbot|petalbot).*"
        }
      ],
      "mitigate": {
        "action": "deny"
      }
    },
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "\\*/\\*"
        }
      ],
      "missing": [
        {
          "type": "header",
          "key": "accept-language"
        }
      ],
      "mitigate": {
        "action": "deny"
      }
    }
  ]
}

